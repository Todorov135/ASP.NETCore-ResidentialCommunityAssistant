{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "PasswordManagement": {
    "RequireConfirmedAccount": false,
    "RequireDigit": false,
    "RequiredLength": 1,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": false
  }
}
